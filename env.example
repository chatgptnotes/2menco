# BETTROI BOS Environment Configuration
# Copy this file to .env and fill in your actual values

# Application Configuration
VITE_APP_NAME="BETTROI BOS"
VITE_APP_VERSION="1.0.0"
VITE_APP_ENV="development" # development, staging, production

# API Configuration
VITE_API_BASE_URL="http://localhost:3001/api"
VITE_API_TIMEOUT="30000"

# Authentication & Security
VITE_AUTH_ENABLED="true"
VITE_JWT_SECRET="your-jwt-secret-key-here"
VITE_SESSION_TIMEOUT="3600" # seconds

# Database Configuration
VITE_DB_HOST="localhost"
VITE_DB_PORT="5432"
VITE_DB_NAME="bettroi_bos"
VITE_DB_USER="postgres"
VITE_DB_PASSWORD="your-database-password"

# External Service APIs
VITE_OPENAI_API_KEY="sk-your-openai-api-key"
VITE_STRIPE_PUBLIC_KEY="pk_test_your-stripe-key"
VITE_STRIPE_SECRET_KEY="sk_test_your-stripe-secret"

# Email Configuration
VITE_SMTP_HOST="smtp.gmail.com"
VITE_SMTP_PORT="587"
VITE_SMTP_USER="your-email@gmail.com"
VITE_SMTP_PASSWORD="your-app-password"

# File Storage
VITE_STORAGE_TYPE="local" # local, s3, gcs
VITE_S3_BUCKET="your-s3-bucket-name"
VITE_S3_REGION="us-east-1"
VITE_S3_ACCESS_KEY="your-s3-access-key"
VITE_S3_SECRET_KEY="your-s3-secret-key"

# Monitoring & Analytics
VITE_ANALYTICS_ENABLED="true"
VITE_GOOGLE_ANALYTICS_ID="GA-XXXXXXXXX"
VITE_SENTRY_DSN="your-sentry-dsn"

# Feature Flags
VITE_FEATURE_AGENTS_ENABLED="true"
VITE_FEATURE_KPIS_ENABLED="true"
VITE_FEATURE_TASKS_ENABLED="true"
VITE_FEATURE_DOCUMENTS_ENABLED="true"

# Business Configuration
VITE_BUSINESS_NAME="BETTROI"
VITE_BUSINESS_CURRENCY="AED"
VITE_BUSINESS_TIMEZONE="Asia/Dubai"
VITE_TARGET_REVENUE="1000000"
VITE_TARGET_TIMELINE_MONTHS="9"

# Development & Debug
VITE_DEBUG_MODE="true"
VITE_LOG_LEVEL="debug" # debug, info, warn, error
VITE_ENABLE_MOCK_DATA="true"
